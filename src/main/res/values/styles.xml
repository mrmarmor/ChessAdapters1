<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:background">@android:color/transparent</item>
        <item name="colorPrimary">@android:color/darker_gray</item>
        <item name="colorPrimaryDark">@android:color/holo_green_dark</item>
        <item name="colorAccent">@android:color/holo_blue_bright</item>
        <!--item name="android:windowActivityTransitions">true</item-->
        <!-- Customize your theme here. -->
    </style>
      <style name="gridView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:layout_column">8</item>
        <item name="android:columnWidth">40dp</item>
        <item name="android:numColumns">auto_fit</item>
        <item name="android:stretchMode">none</item>
        <item name="android:horizontalSpacing">0dp</item>
        <item name="android:verticalSpacing">0dp</item>
    </style>
</resources>